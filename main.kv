<ExitPopup>:
    FloatLayout:
        Button:
            text: "Takaisin"
            size_hint: 0.4, 0.1
            pos_hint: {'center_x':0.5,'center_y':0.4}
            on_press: root.close_popup()
            
        Button:
            text: "Poistu"
            
            size_hint: 0.4, 0.1
            background_normal: ''
            background_color: 1, 0, 0, 0.9           
            pos_hint: {'center_x':0.5,'center_y':0.2}
            on_release:
                root.on_button_yes()
                
                     

