<MenuScreen>:
    name: 'sivu1'
    canvas.before:
        Rectangle:
            pos: self.pos
            size: self.size
            source: 'taustakuva.png'
    
    MDToolbar:
        title: "GolfApp"
        pos_hint: {'center_x':0.5,'center_y':0.95}
        
        
    GridLayout:
        size_hint: 0.6, 0.4
        pos_hint: {'center_x':0.5,'center_y':0.65}
        cols: 1
        rows: 5
        MDRectangleFlatIconButton:
            icon: "golf"
            text: "Uusi kierros"
            size_hint: 0.7, 0.7
            #pos_hint: {'center_x':0.5,'center_y':1}
            text_color: app.theme_cls.primary_color
            md_bg_color: 0.6, 0.8, 1, 0.05
            halign: "center"
            on_press: 
                root.manager.current = 'sivu2'
                root.manager.transition.direction = 'left' 
            
        MDLabel:
            size_hint: 0.7, 0.2
        
        MDRectangleFlatIconButton:
            icon: "content-save"
            text: "Tallennettu kierros"
            size_hint: 0.7, 0.7
            #pos_hint: {'center_x':0.5,'center_y':1}
            text_color: app.theme_cls.primary_color
            md_bg_color: 0.6, 0.8, 1, 0.05
            on_press: 
                root.manager.current = 'kierrossivu'
                root.manager.transition.direction = 'left' 
        
        MDLabel:
            size_hint: 0.7, 0.2
        
        MDRectangleFlatIconButton:
            icon: "close"
            text: "Poistu"
            size_hint: 0.5, 0.7
            #pos_hint: {'center_x':0.5,'center_y':0.3}
            text_color: app.theme_cls.primary_color
            md_bg_color: 0.6, 0.8, 1, 0.05
            on_press: 
                app.get_running_app().stop()
                
    Image:
        source: "pelaaja.png"
        size_hint: 0.9, 0.9
        pos_hint: {'center_x':0.5,'center_y':0.2}
            

